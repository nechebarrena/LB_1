lista = lb_2D.c  funciones.c lb_2D.h
direccion=./corridas
MKDIR=mkdir

all: $(lista)
	gcc $(lista) -O3 -lm -o lb_2D  
	mv lb_2D $(direccion)
clean:
	rm -f *.o


